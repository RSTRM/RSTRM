{
  "expo": {
    "name": "RSTRM",
    "slug": "RSTRM",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.expo.RSTRM",
      "config": {
        "googleMapsApiKey": "./secrets/process.env.GOOGLE_KEY"
        }
    },
    "android": {
      "package": "io.expo.RSTRM"
    }
  }
}